<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.study.model">
    <select id="loginCase" parameterType="Integer" resultType="com.study.model.LoginCase">
         select * from loginCase
            where id=#{id};
    </select>
    <!--添加用户-->
    <select id="addUserCase" parameterType="Integer" resultType="com.study.model.AddUserCase">
        select * from addUserCase where id = #{id};
    </select>
    <!--获取用户信息-->
    <select id="getUserInfo" parameterType="Integer" resultType="com.study.model.GetUserInfoCase">
        select * from getUserInfoCase where id = #{id};
    </select>
    <!--获取用户列表-->
    <select id="getUserListInfo" parameterType="Integer" resultType="com.study.model.GetUserListInfo">
        select * from getUserListCase where id = #{id};
    </select>
    <!--更新删除用户-->
    <select id="updateUserCase" parameterType="Integer" resultType="com.study.model.UpdateUserCase">
        select * from  updateUserCase where id = #{id};
    </select>
</mapper>